{
  "manifest_version": 3,
  "name": "Authenticate in Google Cloud function",
  "description": "Upload web page screenshots as you take them. POST requests include the source page URL, user's Google identity, custom parameters",
  "version": "0.0.1",
  "icons": {
    "128": "imgs/off.png"
  },
  "background": {
    "service_worker": "sw/sw.js",
    "type": "module"
  },
  "action": {},
  "permissions": [
    "scripting",
    "activeTab",
    "identity"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "key": ""
}